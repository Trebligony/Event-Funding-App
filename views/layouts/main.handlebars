                <ul class="list list-sm">
                  <li><a href="#">Floral Services</a></li>
                  <li><a href="#">Catering & DÃ©cor</a></li>
                  <li><a href="#">Photos & Videos</a></li>
                  <li><a href="#">Venue Selection</a></li>
                </ul>
              </div>
            </div>
          </div>
        </div>
      </div>
      <div class="footer-classic-aside">
        <div class="container">
          <p class="rights"><span>&copy;&nbsp; </span><span
              class="copyright-year"></span><span>&nbsp;</span><span>Happy</span><span>. All rights
              reserved.&nbsp;</span>
            <!--a(href='privacy-policy.html') Privacy Policy-->
            <!--span .-->
            <!--span  Design&nbsp;by&nbsp;#[a(href="https://zemez.io/") Zemez]-->
          </p>
        </div>
      </div>
    </footer>
  </div>
  <div class="snackbars" id="form-output-global"></div>
  <!-- Render script for logged in users only -->
  {{#if logged_in}}
  <script src="/js/logout.js"></script>
  {{/if}}
  <script src="/js/core.min.js"></script>
  <script src="/js/script.js"></script>

  <!-- coded by dyoma-->
</body>

</html>