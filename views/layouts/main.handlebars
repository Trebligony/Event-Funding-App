              <div class="rd-navbar-panel">
                <!-- RD Navbar Toggle-->
                <button class="rd-navbar-toggle" data-rd-navbar-toggle=".rd-navbar-nav-wrap"><span></span></button>
                <!-- RD Navbar Brand-->
                <div class="rd-navbar-brand"><a class="brand" href="/"><img class="brand-logo-dark"
                      src="/images/logo-default-170x54.png" alt="" width="170" height="54" /><img
                      class="brand-logo-light" src="images/logo-inverse-170x54.png" alt="" width="170"
                      height="54" /></a>
                </div>
              </div>
              <div class="rd-navbar-nav-wrap">
                <!-- RD Navbar Nav-->
                <ul class="rd-navbar-nav">

                  <!-- Conditionally render login or logout links -->
                  {{#if logged_in}}
                  <li class="rd-nav-item"><a class="rd-nav-link" href="/#home">Home</a></li>
                  <li class="rd-nav-item"><a class="rd-nav-link" href="/#our-team">Our Team</a></li>
                  <li class="rd-nav-item"><a class="rd-nav-link" href="/#prising">Pricing</a></li>
                  <li class="rd-nav-item"><a class="rd-nav-link" href="/#contact-us">Contact Us</a></li>
                  <li class="rd-nav-item"><a class="rd-nav-link" href="/profile">Dashboard</a></li>
                  <li class="rd-nav-item"><a class="button button-lg button-icon button-icon-left button-primary" id="logout" href="#"><span class="icon mdi mdi-logout"></span>Logout</a></li>

                  {{else}}
                  <li class="rd-nav-item"><a class="rd-nav-link" href="/#home">Home</a></li>
                  <li class="rd-nav-item"><a class="rd-nav-link" href="/#our-team">Our Team</a></li>
                  <li class="rd-nav-item"><a class="rd-nav-link" href="/#prising">Pricing</a></li>
                  <li class="rd-nav-item"><a class="rd-nav-link" href="/#contact-us">Contact Us</a></li>
                  <li class="rd-nav-item"><a class="button button-lg button-icon button-icon-left button-primary" href="/login"><span class="icon mdi mdi-login"></span>Login</a></li>

                  {{/if}}
                </ul>
              </div>
            </div>
          </div>
          <div class="rd-navbar-placeholder"></div>
        </nav>
      </div>
    </header>

    <!-- Render the sub layout -->
    {{{ body }}}

